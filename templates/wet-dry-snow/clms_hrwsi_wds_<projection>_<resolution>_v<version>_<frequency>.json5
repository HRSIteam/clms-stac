{
    "type": "Collection",
    "stac_version": "1.1.0",
    "stac_extensions": [
        "https://stac-extensions.github.io/eo/v2.0.0/schema.json",
        "https://stac-extensions.github.io/grid/v1.1.0/schema.json", /* REQUIRED IF APPLICABLE */
        "https://stac-extensions.github.io/processing/v1.2.0/schema.json", /* REQUIRED */
        "https://stac-extensions.github.io/product/v1.0.0/schema.json", /* REQUIRED */
        "https://stac-extensions.github.io/projection/v2.0.0/schema.json", /* REQUIRED */
        "https://stac-extensions.github.io/classification/v2.0.0/schema.json",
        "https://stac-extensions.github.io/version/v1.2.0/schema.json",
        "https://stac-extensions.github.io/contacts/v1.0.0/schema.json"
    ],
    "id": "clms_hrwsi_wds_<projection>_<resolution>_v<version>_<frequency>", /* There is an ongoing discusion if the id should include the type of dile, coq or nc. */
    "title": "SAR Wet/Dry Snow 2016-present (raster 60 m), Europe, daily",
    "description": "The SAR Wet/Dry Snow (WDS) product is generated in near real-time for selected high mountain areas at European scale based on C-band SAR satellite data from the Sentinel-1 constellation. The product provides wet/dry snow state classification for high mountain areas with a spatial resolution of 60 m x 60 m.",
    "license": "other",
    "extent": {
        "spatial": {
            "bbox": [
                [
                    <lon_min>,
                    <lat_min>,
                    <lon_max>,
                    <lat_max>
                ]
            ] /* bbox defined in metadata file */
        },
        "temporal": {
            "interval": [
                [
                    "YYYY-MM-DDT00:00:00.000Z",
                    null
                ]
            ] /* Begin date in metadata file */
        }
    },
    "providers":[
        {
            "url":"https://land.copernicus.eu",
            "name":"EEA",
            "roles":["producer"]
        },
        {
            "url":"https://commission.europa.eu/",
            "name":"European Commission",
            "roles":["licensor"]
        },
        {
            "url":"https://magellium.com/",
            "name":"Magellium",
            "roles":["processor"]
        },
        {
            "url":"https://cloudferro.com/",
            "name":"CloudFerro",
            "roles":["host"]
        }
    ],
    "version": "<number>",
    "gsd": 60,
    "summaries": {
        "grid:code": [
            "<GRID_CODE_1>",
            "<GRID_CODE_2>"
        ], /* REQUIRED IF APPLICABLE. https://github.com/stac-extensions/grid#gridcode  */
        "gsd": [
            60
        ], /* REQUIRED. Ground Sample Distance at the sensor, in meters (m), must be greater than 0. https://github.com/radiantearth/stac-spec/blob/master/commons/common-metadata.md#gsd  */
        "processing:facility": [
            "Magellium"
        ], /* REQUIRED. The name of the facility that produced the data. For example, Copernicus S1 Core Ground Segment - DPA for product of Sentinel-1 satellites. */
        "processing:level": [
            "<PROCESSIN_LEVEL_1>"
        ], /* REQUIRED. https://github.com/stac-extensions/processing?tab=readme-ov-file#suggested-processing-levels */
        "product:type": [
            "<PRODUCT_TYPE_1>"
        ], /* REQUIRED. The product type., */
        "product:timeliness": [
            "<TIMELINESS>"
        ], /* REQUIRED. The average expected timeliness of the product as an ISO 8601 Duration., */
        "product:timeliness_category": [
            "<TIMELINESS_CATEGORY1>"
        ],
        "proj:code": [
            "<PROJ_CODE_1>",
            "<PROJ_CODE_2>"
        ] /* REQUIRED Authority and specific code of the data source (e.g., EPSG:3857) NULL for data in satellite projection. */
    },
    "links": [
        {
            "rel": "root",
            "href": "<CDSE_COLLECTION_LINK>",
            "type": "application/json"
        },
        {
            "rel": "items",
            "href": "<CDSE_COLLECTION_LINK>/items",
            "type": "application/geo+json"
        },
        {
            "rel": "license",
            "href": "https://land.copernicus.eu/en/data-policy",
            "type": "text/html",
            "title": "Legal notice on the use of Copernicus Land Monitoring Service (CLMS) data"
        }
    ],
    "keywords": [
        "ice", /* From GEMET in Metadat pdf */
        "land cover",
        "landscape alteration",
        "monitoring",
        "climate change impact",
        "snow",
        "HR-WSI",
        "CLMS"
    ],
    "contacts": [
        {
            "name": "Copernicus Land Monitoring Service",
            "organization": "European Environment Agency",
            "roles": ["producer"],
            "emails": [
               	{
              		"value":"copernicus@eea.europa.eu",
              		"roles":[
             			"contact point"
              		]
               	}
           	],
            "phones": [
               	{
              		"value":"+45 33 36 71 00",
              		"roles":[
             			"contact point"
              		]
          		}
           	],
            "links": [
               	{
              		"href":"https://land.copernicus.eu/",
         			"rel":"about",
         			"type":"text/html",
         			"title":"Homepage of the Copernicus Land Monitoring Service"
               	}
           	]
        }
    ],
    "item-assets": {
        "Product": {
            "file:checksum": "<CHECKSUM>",/* must be multihash encoded */
            "file:size": "<NUMBER>",
            "roles": [
                "data",
                "metadata",
                "quality",
                "archive"
            ],
            "title": "TAR product",
            "type": "application/tar"
        },
        "MDT": {
            "title": "Product metadata",
            "description": "Product metadata",
            "type": "application/xml",
            "roles": [
                "metadata"
            ],
            "file:checksum": "<CHECKSUM_MTD>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_MTD.xml",
            "file:size": "<SIZE_MTD>"
        },
        "SSC": {
            "title": "Snow State Classification (SSC)",
            "description": "Snow State Classification (wet/dry)",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": [
                "data"
            ],
            "data_type": "uint8",
            "nodata": 255,
            "classification:classes": "<CLASS_OBJECT>", /* classififcation extension */
            "file:checksum": "<CHECKSUM_SSC>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../wds_<tile>_<datetime>_SSC.tif",
            "file:size": "<SIZE_SSC>"
        },
        "SSC-QA": {
            "title": "SSC Quality Assessment",
            "description": "Quality layer for SSC bands",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": [
                "quality",
                "data"
            ],
            "data_type": "uint8",
            "file:checksum": "<CHECKSUM_SSC_QA>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../wds_<tile>_<datetime>_SSC-QA.tif",
            "file:size": "<SIZE_SSC_QA>"
        },
        "QAFLAGS": {
            "title": "Quality Flags (QAFLAGS)",
            "description": "Pixel-level quality flags (bitfield layer)",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": [
                "quality",
                "data"
            ],
            "data_type": "uint8",
            "classification:classes": "<CLASS_OBJECT>", /* classififcation extension */
            "file:checksum": "<CHECKSUM_QAFLAGS>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../wds_<tile>_<datetime>_QAFLAGS.tif",
            "file:size": "<SIZE_QAFLAGS>"
        }
    }
}
