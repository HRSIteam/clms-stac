    "assets": {
        "Product": {
            "href":"<PRODUCT_BUCKET_PATH>", /*s3://CLMS_PRODUCER_BUCKET_PATH/YYYY/MM/DD/PRODUCT_NAME.tar>" where YYYY/MM/DD <- year/month/date of a product upload */
            "file:checksum": "<CHECKSUM>",/* must be multihash encoded */
            "file:size": "<SIZE>", /* specified in bytes */
            "roles": [
                "data",
                "metadata",
                "quality",
                "archive"
            ],
            "title": "TAR product",
            "type": "application/tar"
        },
        "MTD": {
            "title": "Product metadata",
            "description": "Product metadata",
            "type": "application/xml",
            "roles": ["metadata"],
            "file:checksum": "<CHECKSUM_MTD>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/<PRODUCT_ID>_MTD.xml",
            "file:size": "<SIZE_MTD>"
        },
        "WSM": {
            "title": "Wet Snow classification in high Mountains areas",
            "description": "Wet snow cover in high mountain areas",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint8",
            "nodata": 255,
            "classification:classes": [
            {
              "value": 110,
              "name": "wet_snow",
              "description": "Wet snow",
            },
            {
              "value": 125,
              "name": "dry_patchy_or_no_snow",
              "description": "Dry snow or snow-free or patchy snow",
            },
            {
              "value": 200,
              "name": "radar_geometric_distorsions",
              "description": "Radar Shadow, layover, or foreshortening (masked)",
            },
            {
              "value": 210,
              "name": "water",
              "description": "Water (masked)",
            },
            {
              "value": 220,
              "name": "forest",
              "description": "Forest (masked)",
            },
            {
              "value": 230,
              "name": "urban_areas",
              "description": "Urban areas (masked)",
            },
            {
              "value": 240,
              "name": "no_mountain",
              "description": "Non-mountain areas (masked)",
            }
            ],
            "file:checksum": "<CHECKSUM_WSM>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/<PRODUCT_ID>_WSM.tif",
            "file:size": "<SIZE_WSM>"
        },
        "WSM-QA": {
            "title": "Quality layer for the wet snow classification (WSM) layer",
            "description": "Quality layer providing basic assessment of the wet snow classification layer.",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["quality","data"],
            "data_type": "uint8",
            "nodata": 255,
