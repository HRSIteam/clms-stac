{
    "type": "Collection",
    "stac_version": "1.1.0",
    "stac_extension": [
        "https://stac-extensions.github.io/grid/v1.1.0/schema.json", /* REQUIRED IF APPLICABLE */
        "https://stac-extensions.github.io/processing/v1.2.0/schema.json", /* REQUIRED */
        "https://stac-extensions.github.io/product/v1.0.0/schema.json", /* REQUIRED */
        "https://stac-extensions.github.io/projection/v2.0.0/schema.json", /* REQUIRED */
        "https://stac-extensions.github.io/classification/v2.0.0/schema.json",
        "https://stac-extensions.github.io/version/v1.2.0/schema.json",
        "https://stac-extensions.github.io/contacts/v1.0.0/schema.json"
    ],
    "id": "clms_hrwsi_fsc_<projection>_<resolution>_v<version>_<frequency>",
    "title": "Fractional Snow Cover 2016-present (raster 20 m), Europe, daily",
    "description": "Fractional snow cover maps showing FSCTOC and FSCOG",
    "license": "other",
    "extent": {
        "spatial": {
            "bbox": [
                [
                    <lon_min>,
                    <lat_min>,
                    <lon_max>,
                    <lat_max>
                ]
            ] /* https://github.com/radiantearth/stac-spec/blob/master/collection-spec/collection-spec.md#spatial-extent-object */
        },
        "temporal": {
            "interval": [
                [
                    "YYYY-MM-DDT00:00:00.000Z",
                    null /* null if collection is still being added */
                ]
            ] /* Begin date in metadata file */
        }
    },
    "providers":[
        {
            "url":"https://land.copernicus.eu",
            "name":"EEA",
            "roles":["producer"]
        },
        {
            "url":"https://commission.europa.eu/",
            "name":"European Commission",
            "roles":["licensor"]
        },
        {
            "url":"https://magellium.com/",
            "name":"Magellium",
            "roles":["processor"]
        },
        {
            "url":"https://cloudferro.com/",
            "name":"CloudFerro",
            "roles":["host"]
        }
    ],
    "version": "<number>",
    "gsd": 20,
    "summaries": {
        "grid:code": [
            "<GRID_CODE_1>",
            "<GRID_CODE_2>"
        ], /* REQUIRED IF APPLICABLE. https://github.com/stac-extensions/grid#gridcode  */
        "gsd": [
            20
        ], /* REQUIRED. Ground Sample Distance at the sensor, in meters (m), must be greater than 0. https://github.com/radiantearth/stac-spec/blob/master/commons/common-metadata.md#gsd  */
        "processing:facility": [
            "Magellium"
        ], /* REQUIRED. The name of the facility that produced the data. For example, Copernicus S1 Core Ground Segment - DPA for product of Sentinel-1 satellites. */
        "processing:level": [
            "<PROCESSIN_LEVEL>"
        ], /* REQUIRED. https://github.com/stac-extensions/processing?tab=readme-ov-file#suggested-processing-levels */
        "product:type": [
            "<PRODUCT_TYPE>"
        ], /* REQUIRED. The product type., */
        "product:timeliness": [
            "<TIMELINESS>"
        ], /* REQUIRED. The average expected timeliness of the product as an ISO 8601 Duration., */
        "product:timeliness_category": [
            "<TIMELINESS_CATEGORY>"
        ],
        "proj:code": [
            "<PROJ_CODE_1>",
            "<PROJ_CODE_2>"
        ]
    },
    "links": [
        {
            "rel": "root",
            "href": "<CDSE_COLLECTION_LINK>",
            "type": "application/json"
        },
        {
            "rel": "items",
            "href": "<CDSE_COLLECTION_LINK>/items",
            "type": "application/geo+json"
        },
        {
            "rel": "license",
            "href": "https://land.copernicus.eu/en/data-policy",
            "type": "text/html",
            "title": "Legal notice on the use of Copernicus Land Monitoring Service (CLMS) data"
        }
    ],
    "keywords": [
        "ice", /* From GEMET in Metadat pdf */
        "monitoring",
        "snow",
        "land cover",
        "climate change impact",
        "landscape alteration",
        "fractional snow cover",
        "HR-WSI",
        "CLMS"
    ],
    "contacts": [
        {
            "name": "Copernicus Land Monitoring Service",
            "organization": "European Environment Agency",
            "emails": [
               	{
              		"value":"copernicus@eea.europa.eu",
              		"roles":[
             			"contact point"
              		]
               	}
           	],
            "phones": [
               	{
              		"value":"+45 33 36 71 00",
              		"roles":[
             			"contact point"
              		]
          		}
           	],
            "links": [
               	{
              		"href":"https://land.copernicus.eu/",
         			"rel":"about",
         			"type":"text/html",
         			"title":"Homepage of the Copernicus Land Monitoring Service"
               	}
           	]
        }
    ],
    "item-assets": {
        "Product": {
            "file:checksum": "<CHECKSUM>",/* must be multihash encoded */
            "file:size": "<NUMBER>",
            "roles": [
                "data",
                "metadata",
                "archive",
                "quality",
                "mask"
            ],
            "title": "TAR product",
            "type": "application/tar"
        },
        "MDT": {
            "title": "Product metadata",
            "description": "Product metadata",
            "type": "application/xml",
            "roles": ["metadata"],
            "file:checksum": "<CHECKSUM_MTD>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_MTD.xml",
            "file:size": "<SIZE_MTD>"
        },
        "FSCTOC": {
            "title": "Forest/Non-Forest Classification (FSCTOC)",
            "description": "Binary forest/non-forest classification",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint8",
            "nodata": 255,
            "file:checksum": "<CHECKSUM_FSCTOC>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_FSCTOC.tif",
            "file:size": "<SIZE_FSCTOC>"
        },
        "FSCOG": {
            "title": "Forest Cover Fraction (FSCOG)",
            "description": "Forest cover fraction [0â€“100]",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint8",
            "nodata": 255,
            "file:checksum": "<CHECKSUM_FSCOG>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_FSCOG.tif",
            "file:size": "<SIZE_FSCOG>"
        },
        "FSCTOC-QA": {
            "title": "FSCTOC Quality Assessment",
            "description": "Quality layer for FSCTOC",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["quality","data"],
            "data_type": "uint8",
            "classification:classes": "<CLASS_OBJECT>", /* classififcation extension */
            "file:checksum": "<CHECKSUM_FSCTOC_QA>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_FSCTOC-QA.tif",
            "file:size": "<SIZE_FSCTOC_QA>"
        },
        "FSCOG-QA": {
            "title": "FSCOG Quality Assessment",
            "description": "Quality layer for FSCOG",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["quality","data"],
            "data_type": "uint8",
            "classification:classes": "<CLASS_OBJECT>", /* classififcation extension */
            "file:checksum": "<CHECKSUM_FSCOG_QA>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_FSCOG-QA.tif",
            "file:size": "<SIZE_FSCOG_QA>"
        },
        "QAFLAGS": {
            "title": "Quality Flags (QAFLAGS)",
            "description": "Bitmask layer with pixel quality flags",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["quality","data"],
            "data_type": "uint8",
            "classification:classes": "<CLASS_OBJECT>", /* classififcation extension */
            "file:checksum": "<CHECKSUM_QAFLAGS>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_QAFLAGS.tif",
            "file:size": "<SIZE_QAFLAGS>"
        },
        "CLD": {
            "title": "Cloud Mask (CLD)",
            "description": "Cloud mask for HRSWI product",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["mask","data"],
            "data_type": "uint8",
            "nodata": 255,
            "classification:classes": "<CLASS_OBJECT>", /* classififcation extension */
            "file:checksum": "<CHECKSUM_CLD>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_CLD.tif",
            "file:size": "<SIZE_CLD>"
        },
        "NDSI": {
            "title": "Normalized Difference Snow Index (NDSI)",
            "description": "NDSI scaled or categorized as uint8",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint8",
            "nodata": 255,
            "file:checksum": "<CHECKSUM_NDSI>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_NDSI.tif",
            "file:size": "<SIZE_NDSI>"
        }
    }
