        "spatial": {
            "bbox": [
                [
                    "<lon_min>", /* -26 */
                    "<lat_min>", /* 33 */
                    "<lon_max>", /* 48 */
                    "<lat_max>" /* 72 */
                ]
            ], /* bbox for EEA38+UK */
        },
        "temporal": {
            "interval": [
                [
                    "YYYY-MM-DDT00:00:00.000Z", /* 2016-08-01T00:00:00.000Z */
