{
    "type": "Feature",
    "stac_version": "1.1.0",
    "stac_extensions": [
        "https://stac-extensions.github.io/file/v2.1.0/schema.json",
        "https://stac-extensions.github.io/grid/v1.1.0/schema.json",
        "https://stac-extensions.github.io/processing/v1.2.0/schema.json",
        "https://stac-extensions.github.io/product/v1.0.0/schema.json",
        "https://stac-extensions.github.io/projection/v2.0.0/schema.json",
        "https://stac-extensions.github.io/raster/v2.0.0/schema.json",
        "https://stac-extensions.github.io/classification/v2.0.0/schema.json", 
    ],
    "id": "<PRODUCT_ID>", 
    "bbox": [
        "<WEST_BOUNDING_LONGITUDE>",
        "<SOUTH_BOUNDING_LATITUDE>",
        "<EAST_BOUNDING_LONGITUDE>",
        "<NORTH_BOUNDING_LATITUDE>"
    ],
    "geometry": {
        "type": "Polygon",
        "coordinates": [
            [
                [
                    "<WEST_BOUNDING_LONGITUDE>",
                    "<SOUTH_BOUNDING_LATITUDE>"
                ],
                [
                    "<EAST_BOUNDING_LONGITUDE>",
                    "<SOUTH_BOUNDING_LATITUDE>"
                ],
                [
                    "<EAST_BOUNDING_LONGITUDE>",
                    "<NORTH_BOUNDING_LATITUDE>"
                ],
                [
                    "<WEST_BOUNDING_LONGITUDE>",
                    "<NORTH_BOUNDING_LATITUDE>"
                ],
                [
                    "<WEST_BOUNDING_LONGITUDE>",
                    "<SOUTH_BOUNDING_LATITUDE>"
                ]
            ]
        ]
    },
    "properties": {
        "created": "<CREATION_DATETIME>",/*When the JSON was generated by the CLMS producer. Here datetime.now(). Format 2025-06-21T00:00:00.000000Z.*/
        "datetime": "<DATETIME>", /* Indicates the date the data is meant to represent, not necessarily when it was acquired or generated. same as the date indicated in the product name with the format 2025-06-21T00:00:00.000000Z. */
        "description": "The Sentinel-1 and Sentinel-2 Snow Phenology (SP S1+ S2) product is generated once a year over high-mountain areas at European scale, based on daily cumulative Gap-Filled Fractional Snow Cover (GFSC) products calculated from Sentinel-2 optical and Sentinel-1 radar data. This product describes the snow season in terms of temporality as it provides, within an hydrological year and for each pixel, the number of days with snow cover, as well as the first and the last day of the longest observed snow period. The hydrological year starts from 1st September. The product has a spatial resolution of 60 m x 60 m, as does the input GFSC product.",
        "start_datetime": "<START_DATETIME>", /* start of the hydrological year, with the format xxx-09-01T00:00:00.000000Z. */
        "end_datetime": "<END_DATETIME>", /* start of the hydrological year, with the format xxxx-08-31T23:59:59.999999Z. */
        "gsd": 60, /* in meters */
        "grid:code": "MGRS-<TILE_ID>", /* eg. MGRS-32TNS */
        "instruments": [
            "msi", "sar"
        ],
        "platform": "multiplatform", 
        "constellation": "multimission",
        "processing:facility": "<PROCESSING_CENTER>", /* here Magellium/WEkEO */
        "processing:level": "L3",
        "processing:datetime": "<PROCESSING_DATETIME>", /* with the format 2025-06-21T00:00:00.000000Z. */
        "processing:version": "<PRODUCT_VERSION>", /* eg. v0.0 */
        "product:type": "snow_phenology",
        "product:timeliness": "PT1M",/* ISO 8601 duration */
        "product:timeliness_category": "NTC",
        "proj:code": "EPSG:<EPSG>", /* eg. EPSG:326XX where XX indicates the UTM zone, also corresponds to the first 2 digits of a tile name. */ 
        "proj:shape": [
            "<PROJ_SHAPE_HEIGHT>",
            "<PROJ_SHAPE_WIDTH>"
        ], /* gdalinfo size result */
        "proj:transform": [
            "<PROJ_TRANSFORM_0>",
            "<PROJ_TRANSFORM_1>",
            "<PROJ_TRANSFORM_2>",
            "<PROJ_TRANSFORM_3>",
            "<PROJ_TRANSFORM_4>",
            "<PROJ_TRANSFORM_5>",
            "<PROJ_TRANSFORM_6>",
            "<PROJ_TRANSFORM_7>",
            "<PROJ_TRANSFORM_8>"
        ], /* The affine transformation coefficients for the default grid. Obtained from GDAL(GetGeoTransform, requires re-ordering) or the Rasterio (Transform). */
    },
    "collection": "<COLLECTION_NAME>", /* name of the collection JSON file */
    "links": [
        {
            "rel": "collection",
            "href": "<CDSE_COLLECTION_LINK>", /* this will be added by the CDSE */
            "type": "application/json"
        },
        {
            "rel": "parent",
            "href": "<CDSE_COLLECTION_LINK>",
            "type": "application/json"
        },
        {
            "rel": "root",
            "href": "https://stac.dataspace.copernicus.eu/v1/",
            "type": "application/json"
        },
        {
            "rel": "license",
            "href": "https://land.copernicus.eu/en/data-policy",
            "title": "Legal notice on the use of CLMS data"
        }
    ],
    "assets": {
        "Product": {
            "file:checksum": "<CHECKSUM>",/* must be multihash encoded */
            "file:size": "<NUMBER>", /* specified in bytes */
            "roles": [
                "data",
                "metadata",
                "quality",
                "archive"
            ],
            "title": "TAR product",
            "type": "application/tar"
        },
        "MTD": {
            "title": "Product metadata",
            "description": "Product metadata",
            "type": "application/xml",
            "roles": ["metadata"],
            "file:checksum": "<CHECKSUM_MTD>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_MTD.xml",
            "file:size": "<SIZE_MTD>"
        },
        "QAFLAGS": {
            "title": "Quality Flags (QAFLAGS)",
            "description": "Bitmask layer with pixel quality flags",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["quality","data"],
            "data_type": "uint8",
            "classification:bitfields": [
            {
              "name": "snow_cover_duration_too_low",
              "description": "Indicates pixels with no sufficient snowfall throughout the year to allow for an accurate estimate of the snow season.",
              "offset": 4,
              "length": 1,
              "classes": [
                {
                  "name": "not_snow_cover_duration_too_low",
                  "description": "The pixel receives sufficient snowfall throughout the year to characterize the snow season.",
                  "value": 0
                },
                {
                  "name": "snow_cover_duration_too_low",
                  "description": "The pixel does not receive sufficient snowfall throughout the year to allow for an accurate estimate of the snow season.",
                  "value": 1
                }
              ]
            },
            {
              "name": "tree_cover_duration_too_high",
              "description": "Indicates whether the tree cover density is too high for an accurate snow detection.",
              "offset": 5,
              "length": 1,
              "classes": [
                {
                  "name": "not_tree_cover_duration_too_high",
                  "description": "The tree cover density is not too high for an accurate snow detection.",
                  "value": 0
                },
                {
                  "name": "tree_cover_duration_too_high",
                  "description": "The tree cover density is too high for an accurate snow detection.",
                  "value": 1
                }
              ]
            }, 
            {
              "name": "wet_snow_detection_area",
              "description": "Indicates whether the pixel is part of the area in which wet snow detection is applied (high elevation, non-forested, non-urban, non-water areas).",
              "offset": 7,
              "length": 1,
              "classes": [
                {
                  "name": "not_wet_snow_detection_area",
                  "description": "The pixel is not part of the area in which wet snow detection is applied.",
                  "value": 0
                },
                {
                  "name": "wet_snow_detection_area",
                  "description": "The pixel is part of the area in which wet snow detection is applied.",
                  "value": 1
                }
              ]
            }
            ],
            "file:checksum": "<CHECKSUM_QAFLAGS>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_QAFLAGS.tif",
            "file:size": "<SIZE_QAFLAGS>"
        },
        "SCD": {
            "title": "Snow Cover Duration (days)",
            "description": "Number of days of snow cover over the hydrological year (between 0 and 366)",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint16",
            "nodata": 65535,
            "classification:classes": [
            {
              "value": 420,
              "name": "inland_water",
              "description": "Inland water",
            }
            ],
            "file:checksum": "<CHECKSUM_SCD>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_SCD.tif",
            "file:size": "<SIZE_SCD>"
        },
        "SCO": {
            "title": "Snow Cover Onset-date (day of the year)",
            "description": "Snow cover onset-date defined as the first day of the longest snow cover period. Date expressed as the number of days from the beginning of the hydrological year (1st September = day 0).",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint16",
            "nodata": 65535,
            "classification:classes": [
            {
              "value": 420,
              "name": "inland_water",
              "description": "Inland water",
            }
            ],
            "file:checksum": "<CHECKSUM_SCO>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_SCO.tif",
            "file:size": "<SIZE_SCO>"
        },
        "SCM": {
            "title": "Snow Cover Melt out date (day of the year)",
            "description": "Snow cover melt out date defined as the last day of the longest snow cover period. Date expressed as the number of days from the beginning of the hydrological year (1st September = day 0).",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint16",
            "nodata": 65535,
            "classification:classes": [
            {
              "value": 420,
              "name": "inland_water",
              "description": "Inland water",
            }
            ],
            "file:checksum": "<CHECKSUM_SCM>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_SCM.tif",
            "file:size": "<SIZE_SCM>"
        },
        "NCSO": {
            "title": "Number of days with clear sky observations over the hydrological year used in the interpolation of the snow cover time series.",
            "description": "Number of days with clear sky observations used in the interpolation of the snow cover time series (between 0 and 366).",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint16",
            "nodata": 65535,
            "classification:classes": [
            {
              "value": 420,
              "name": "inland_water",
              "description": "Inland water",
            }
            ],
            "file:checksum": "<CHECKSUM_NCSO>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_NCSO.tif",
            "file:size": "<SIZE_NCSO>"
        },
        "NWSO": {
            "title": "Number of days with S1-based wet snow observations over the hydrological year used in the interpolation of the snow cover time series",
            "description": "Number of days with S1-based wet snow observations used in the interpolation of the snow cover time series (used where S2-based observations have gaps) (between 0 and 366).",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint16",
            "nodata": 65535,
            "classification:classes": [
            {
              "value": 420,
              "name": "inland_water",
              "description": "Inland water",
            }
            ],
            "file:checksum": "<CHECKSUM_NWSO>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_NWSO.tif",
            "file:size": "<SIZE_NWSO>"
        },
        "SCD-QA": {
            "title": "Quality layer for the Snow Cover Duration (SCD) layer",
            "description": "Quality layer providing basic assessment of the Snow Cover Duration layer.",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["quality","data"],
            "data_type": "uint16",
            "nodata": 65535,
            "classification:classes": [
            {
              "value": 0,
              "name": "high_quality",
              "description": "High quality",
            },
            {
              "value": 1,
              "name": "medium_quality",
              "description": "Medium quality"
            },
            {
              "value": 2,
              "name": "low_quality",
              "description": "Low quality",
            },
            {
              "value": 3,
              "name": "minimal_quality",
              "description": "Minimal quality",
            },
            {
              "value": 420,
              "name": "inland_water",
              "description": "Inland water",
            }
            ],
            "file:checksum": "<CHECKSUM_SCD-QA>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_SCD-QA.tif",
            "file:size": "<SIZE_SCD-QA>"
        },
        "SCO-QA": {
            "title": "Quality indicator associated with the Snow Cover Onset (SCO) date",
            "description": "Number of days between the estimated snow onset date and the closest snow observation date (between 0 and 366).",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint16",
            "nodata": 65535,
            "file:checksum": "<CHECKSUM_SCO-QA>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_SCO-QA.tif",
            "file:size": "<SIZE_SCO-QA>"
        },
        "SCM-QA": {
            "title": "Quality indicator associated with the Snow Cover Melt out (SCM) date",
            "description": "Number of days between the estimated snow melt out date and the closest snow observation date (between 0 and 366).",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint16",
            "nodata": 65535,
            "file:checksum": "<CHECKSUM_SCM-QA>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/../<PRODUCT_ID>_SCM-QA.tif",
            "file:size": "<SIZE_SCM-QA>"
        }
    }
}
