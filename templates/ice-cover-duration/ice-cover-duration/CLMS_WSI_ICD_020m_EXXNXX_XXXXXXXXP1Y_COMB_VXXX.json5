            "file:local_path": "<PRODUCT_ID>/<PRODUCT_ID>_MTD.xml",
            "file:size": "<SIZE_MTD>"
        },
        "ICD": {
            "title": "Ice Cover Duration (days)",
            "description": "Number of days of ice cover on water over the hydrological year (between 0 and 366)",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint16",
            "nodata": 65535,
            "file:checksum": "<CHECKSUM_ICD>",/* must be multihash encoded */
            "file:local_path": "<PRODUCT_ID>/<PRODUCT_ID>_ICD.tif",
            "file:size": "<SIZE_ICD>"
        },
        "NOBS1": {
            "title": "Number of days with Sentinel-1 based observations over the hydrological year.",
            "description": "Number of days with Sentinel-1 based observations used in the interpolation of the ice cover time series (between 0 and 366).",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint16",
            "nodata": 65535,
            "file:checksum": "<CHECKSUM_NOBS1>",/* must be multihash encoded */
