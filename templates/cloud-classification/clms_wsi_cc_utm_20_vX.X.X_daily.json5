{
    "type": "Collection",
    "stac_version": "1.1.0",
    "stac_extension": [
        "https://stac-extensions.github.io/grid/v1.1.0/schema.json",
        "https://stac-extensions.github.io/processing/v1.2.0/schema.json",
        "https://stac-extensions.github.io/product/v1.0.0/schema.json",
        "https://stac-extensions.github.io/projection/v2.0.0/schema.json",
        "https://stac-extensions.github.io/classification/v2.0.0/schema.json",
        "https://stac-extensions.github.io/version/v1.2.0/schema.json",
        "https://stac-extensions.github.io/contacts/v1.0.0/schema.json",
        "https://stac-extensions.github.io/item-assets/v1.0.0/schema.json"
    ],
    "id": "clms_wsi_cc_utm_20_v<version>_daily",
    "title": "Cloud Classification 2016-present (raster 20 m), Europe, daily",
    "description": "The Cloud Classification (CC) product is generated in NRT for the entire EEA38+UK area based on optical satellite data from the Sentinel-2 constellation. The product provides the extent of clouds and cloud shadows with a pixel spacing of 20 m x 20 m. Cloud detection is performed at a 120m spatial resolution.",
    "license": "other", 
    "extent": {
        "spatial": {
            "bbox": [
                [
                    "<lon_min>", /* -26 */
                    "<lat_min>", /* 33 */
                    "<lon_max>", /* 48 */
                    "<lat_max>" /* 72 */
                ]
            ], /* bbox for EEA38+UK */
        },
        "temporal": {
            "interval": [
                [
                    "YYYY-MM-DDT00:00:00.000Z", /* 2016-08-01T00:00:00.000Z */
                    null
                ]
            ], /* Begin date in metadata file */
        }
    },
    "providers":[
        {
            "url":"https://land.copernicus.eu",
            "name":"EEA",
            "roles":["producer"]
        },
        {
            "url":"https://commission.europa.eu/",
            "name":"European Commission",
            "roles":["licensor"]
        },
        {
            "url":"https://magellium.com/",
            "name":"Magellium/CNES",
            "roles":["processor"]
        },
        {
            "url":"https://cloudferro.com/",
            "name":"CloudFerro",
            "roles":["host"]
        }
    ],
    "version": "<version>", /* major version, eg v0.0.0*/
    "gsd": 20,
    "summaries": {
        "grid:code": [
            "<GRID_CODE_1>",
            "<GRID_CODE_2>"
        ], /* REQUIRED IF APPLICABLE. https://github.com/stac-extensions/grid#gridcode . Here list of all the S2 tiles processed over EE38+UK with the format "MGRS-32TNS" */
        "gsd": [
            20
        ], /* REQUIRED. Ground Sample Distance at the sensor, in meters (m), must be greater than 0. If MIN_GSD=MAX_GSD Please provide a single value like "gsd":[VALUE]. https://github.com/radiantearth/stac-spec/blob/master/commons/common-metadata.md#gsd  */
        "processing:facility": [
            "Magellium/WEkEO"
        ], /* REQUIRED. The name of the facility that produced the data. For example, Copernicus S1 Core Ground Segment - DPA for product of Sentinel-1 satellites. */
        "processing:level": [
            "L2", 
        ], /* REQUIRED. https://github.com/stac-extensions/processing?tab=readme-ov-file#suggested-processing-levels */
        "product:type": [
            "cloud_mask"
        ], /* REQUIRED. TBC */
        "product:timeliness": [
            "PT12H",
        ], /* REQUIRED. The average expected timeliness of the product as an ISO 8601 Duration., */
        "product:timeliness_category": [
            "NRT"
        ],
        "proj:code": [
            "EPSG:32625",
            "EPSG:32626",
            "EPSG:32627",
            "EPSG:32628",
            "EPSG:32629",
            "EPSG:32630",
            "EPSG:32631",
            "EPSG:32632",
            "EPSG:32633",
            "EPSG:32634",
            "EPSG:32635",
            "EPSG:32636",
            "EPSG:32637",
            "EPSG:32638"
        ] 
    }, /* REQUIRED Authority and specific code of the data source (e.g., EPSG:3857) NULL for data in satellite projection. */
