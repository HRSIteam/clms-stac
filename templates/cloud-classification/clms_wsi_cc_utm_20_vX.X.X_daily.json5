    "item_assets": {
        "Product": {
            "roles": [
                "data",
                "metadata",
                "quality",
                "archive"
            ],
            "title": "TAR product",
            "type": "application/tar"
        },
        "MTD": {
            "title": "Product metadata",
            "description": "Product metadata",
            "type": "application/xml",
            "roles": ["metadata"],
        },
        "QAFLAGS": {
            "title": "Quality Flags (QAFLAGS)",
            "description": "Bitmask layer with pixel quality flags",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["quality","data"],
            "data_type": "uint8",
            "classification:bitfields": [
            {
              "name": "mono_temporal",
              "description": "Indicates whether cloud pixels were detected through the mono-temporal approach",
              "offset": 0,
              "length": 1,
              "classes": [
                {
                  "name": "not_mono_temporal",
                  "description": "Cloud was not detected through the mono-temporal approach",
                  "value": 0
                },
                {
                  "name": "mono_temporal",
                  "description": "Cloud was detected through the mono-temporal approach",
                  "value": 1
                }
              ]
            },
            {
              "name": "multi_temporal",
              "description": "Indicates whether cloud pixels were detected through the multi-temporal approach",
              "offset": 1,
              "length": 1,
              "classes": [
                {
                  "name": "not_multi_temporal",
                  "description": "Cloud was not detected through the multi-temporal approach",
                  "value": 0
                },
                {
                  "name": "multi_temporal",
                  "description": "Cloud was detected through the multi-temporal approach",
                  "value": 1
                }
              ]
            },
            {
              "name": "thin_cloud",
              "description": "Thin cloud mask",
              "offset": 2,
              "length": 1,
              "classes": [
                {
                  "name": "not_thin_cloud",
                  "description": "The pixel was not identified as thin cloud",
                  "value": 0
                },
                {
                  "name": "thin_cloud",
                  "description": "The pixel was identified as thin cloud",
                  "value": 1
                }
              ]
            },
            {
              "name": "cloud_shadow_from_cloud_in_image",
              "description": "Cloud shadow mask cast from a cloud visible in the image",
              "offset": 3,
              "length": 1,
              "classes": [
                {
                  "name": "not_cloud_shadow_from_cloud_in_image",
                  "description": "The pixel is not a cloud shadow cast from a cloud visible in the image",
                  "value": 0
                },
                {
                  "name": "cloud_shadow_from_cloud_in_image",
                  "description": "The pixel is a cloud shadow cast from a cloud visible in the image",
                  "value": 1
                }
              ]
            },
            {
              "name": "cloud_shadow_from_cloud_outside_image",
              "description": "Cloud shadow mask cast from a cloud located outside the image",
              "offset": 4,
              "length": 1,
              "classes": [
                {
                  "name": "not_cloud_shadow_from_cloud_outside_image",
                  "description": "The pixel is not a cloud shadow cast from a cloud located outside in the image",
                  "value": 0
                },
                {
                  "name": "cloud_shadow_from_cloud_outside_image",
                  "description": "The pixel is a cloud shadow cast from a cloud located outside in the image",
                  "value": 1
                }
              ]
            },
            {
              "name": "water",
              "description": "Water mask",
              "offset": 5,
              "length": 1,
              "classes": [
                {
                  "name": "not_water",
                  "description": "The pixel does not cover a water body",
                  "value": 0
                },
                {
                  "name": "water",
                  "description": "The pixel belongs to a water body",
                  "value": 1
                }
              ]
            },
            ]
        },
        "CC": {
            "title": "Cloud classification",
            "description": "Cloud and cloud shadow classification of the image.",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["data"],
            "data_type": "uint8",
            "nodata": 255,
            "classification:classes": [
            {
              "value": 0,
              "name": "clear",
              "description": "Cloud and cloud shadow-free",
            },
            {
              "value": 1,
              "name": "high_cloud",
              "description": "High cloud"
            },
            {
              "value": 2,
              "name": "other_cloud",
              "description": "Other cloud than high cloud",
            },
            {
              "value": 3,
              "name": "cloud_shadow",
              "description": "Cloud shadow",
            }
            ]
        },
        "CC-QA": {
            "title": "Quality layer for CC",
            "description": "Quality layer providing basic assessment of the Cloud Classification layer quality.",
            "type": "image/tiff; application=geotiff; profile=cloud-optimized",
            "roles": ["quality","data"],
            "data_type": "uint8",
            "classification:classes": [
            {
              "value": 0,
              "name": "high_quality",
              "description": "High quality",
            },
            {
              "value": 1,
              "name": "medium_quality",
              "description": "Medium quality"
            },
            {
              "value": 2,
              "name": "low_quality",
              "description": "Low quality",
            },
            {
              "value": 3,
              "name": "minimal_quality",
              "description": "Minimal quality",
            }
            ]
        }
    }
